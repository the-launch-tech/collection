{
  "name": "polldash-client",
  "version": "0.0.1",
  "description": "Client for PollDash task.",
  "main": "dist/server.js",
  "scripts": {
    "tsc": "tsc",
    "dev": "export NODE_ENV=development && npm-run-all --parallel d:*",
    "d:tsc": "tsc --watch --preserveWatchOutput",
    "d:server": "webpack --config webpack.server.config.js --watch",
    "d:client": "webpack --config webpack.client.config.js --watch",
    "d:start": "nodemon --watch dist --exec \"node dist/server.js\"",
    "prod:build": "export NODE_ENV=production && npm-run-all --parallel p:*",
    "p:tsc": "tsc",
    "p:server": "webpack --config webpack.server.config.prod.js",
    "p:client": "webpack --config webpack.client.config.prod.js",
    "prod:start": "nodemon --watch dist --exec \"node dist/server.js\""
  },
  "author": "Daniel Griffiths",
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "serialize-javascript": "^3.1.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.35",
    "@types/react-cookies": "^0.1.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.7",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/serialize-javascript": "^1.5.0",
    "@types/validator": "^13.0.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.0",
    "terser-webpack-plugin": "^3.0.3",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
