{
    "name": "solid-block-editor-v1",
    "version": "1.0.0",
    "description": "SolidJS Block Editor Library",
    "scripts": {
        "dev:watch": "run-s format lint test dev:build dev:start",
        "dev:start": "vite --force",
        "dev:build": "vite build",
        "lint": "eslint --ignore-path .eslintignore --ext .ts .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "test": "run-s test:browser",
        "test:browser": "jest --coverage --selectProjects BROWSER",
        "debug": "nodemon --inspect-brk"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/the-launch-tech/solid-block-editor.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/the-launch-tech/solid-block-editor/issues"
    },
    "homepage": "https://github.com/the-launch-tech/solid-block-editor#readme",
    "dependencies": {
        "solid-js": "^1.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.17.2",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.16",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "@typescript-eslint/type-utils": "^5.11.0",
        "@typescript-eslint/utils": "^5.11.0",
        "babel-jest": "^27.5.1",
        "babel-preset-solid": "^1.3.6",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-ext": "^0.1.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-utils": "^3.0.0",
        "jest": "^27.5.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.6",
        "postcss-scss": "^4.0.3",
        "prettier": "^2.5.1",
        "sass": "^1.49.7",
        "solid-jest": "^0.2.0",
        "stylelint": "^14.4.0",
        "stylelint-config-recommended-scss": "^5.0.2",
        "stylelint-config-standard-scss": "^3.0.0",
        "ts-node": "^10.5.0",
        "tslib": "^2.3.1",
        "typescript": "^4.4.4",
        "vite": "^2.5.7",
        "vite-plugin-solid": "^2.0.3"
    }
}
